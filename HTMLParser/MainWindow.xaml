<Window x:Class="HTMLParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HTMLParser"
        xmlns:messageListControl="clr-namespace:MessageListControl;assembly=MessageListControl"
        xmlns:xcdg="http://schemas.xceed.com/wpf/xaml/datagrid"
        xmlns:busyControl="clr-namespace:BusyControl;assembly=BusyControl"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <local:ViewModel></local:ViewModel>
    </Window.DataContext>
    <Grid>
       <StackPanel>
            <busyControl:BusyControl  IsBusy="True" MinValue="0" MaxValue="40" Value="20" Description="Analizando.."></busyControl:BusyControl>
            <Button Click="Button_Click">Clickame</Button>
            <Button Click="Button_Click_1">Soy el dos</Button>
        </StackPanel>
        
    </Grid>
</Window>
